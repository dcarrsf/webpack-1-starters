{
  "name": "tape-angular-test",
  "version": "1.0.0",
  "description": "Test with Tape library",
  "main": "index.js",
  "keywords": [
    "Angular, Tape, Webpack"
  ],
  "scripts": {
    "dev": "webpack --watch --config ./webpack.dev.config.js --progress --colors",
    "build": "webpack --config ./webpack.prod.config.js --progress --colors",
    "test": "tape -r ./test/helpers/jsdom.js ./test/**/*.js | faucet",
    "watch": "tape-watch ./test/**/*.js | faucet"
  },
  "dependencies": {
    "angular": "^1.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.6.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.22.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^1.9.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "faucet": "0.0.1",
    "file-loader": "^0.8.5",
    "jsdom": "^7.2.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "tape": "^4.6.3",
    "tape-plus": "^1.0.0",
    "tape-watch": "^2.2.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.1",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "author": "Dan Carr",
  "license": "ISC"
}
